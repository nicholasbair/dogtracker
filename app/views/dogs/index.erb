<% @dogs.each do |dog| %>
  <div>
    <h4>
      <a href="/dogs/<%= dog.id %>">
        <%= dog.name %>
      </a>
    </h4>
  </div>
<% end %>

<h4>Recent Activities</h4>
<% current_user.activities.each do |activity| %>
  <p>
    <a href="/activities/<%= activity.id %>">
      <%= activity.generate_story %>
    </a>
  </p>
<% end %>
