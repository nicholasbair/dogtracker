<row>
  <div class="col-md-3"></div>
  <div class="col-md-6">
    <h1>Edit a dog</h1>
    <form class="" action="/dogs/<%= @dog.id %>" method="post">
      <input type="hidden" name="_method" value="PATCH">
      <div class="form-group">
        <input type="text" class="form-control" name="name" value="<%= @dog.name %>">
      </div>
      <button class="btn btn-success" type="submit" name="submit">Update Dog</button>
    </form>
  </div>
</row>
